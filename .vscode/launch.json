{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "C++ Debug",
            "type": "cppdbg",
            "request": "launch",
            "program": "${workspaceFolder}/linked_list/reverse/cpp/reverse_k_group_with_constraints.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}/linked_list/reverse/cpp",
            "environment": [],
            "externalConsole": false,
            "MIMode": "gdb",
            "miDebuggerPath": "gdb",
            "setupCommands": [
                {
                    "description": "为 gdb 启用整齐打印",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ],
            "preLaunchTask": "C++: g++ build reverse k group"
        },
        {
            "name": "Rust Debug",
            "type": "lldb",
            "request": "launch",
            "program": "${workspaceFolder}/linked_list/reverse/rust/target/debug/reverse_k_group_with_constraints",
            "args": [],
            "cwd": "${workspaceFolder}/linked_list/reverse/rust",
            "stopOnEntry": false,
            "environment": [],
            "externalConsole": false,
            "MIMode": "lldb",
            "preLaunchTask": "Rust: cargo run reverse k group"
        },
        {
            "name": "Go Debug",
            "type": "go",
            "request": "launch",
            "mode": "debug",
            "program": "${workspaceFolder}/linked_list/reverse/go",
            "cwd": "${workspaceFolder}/linked_list/reverse/go",
            "env": {},
            "args": [],
            "showLog": true
        },
        {
            "name": "Python Debug",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/linked_list/reverse/python/reverse_k_group_with_constraints.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/linked_list/reverse/python",
            "args": [],
            "justMyCode": true
        },
        {
            "name": "TypeScript Debug",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/linked_list/reverse/typescript/dist/reverse_k_group_with_constraints.js",
            "cwd": "${workspaceFolder}/linked_list/reverse/typescript",
            "args": [],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "preLaunchTask": "TypeScript: tsc build reverse k group",
            "sourceMaps": true,
            "outFiles": [
                "${workspaceFolder}/linked_list/reverse/typescript/dist/**/*.js"
            ]
        },
        {
            "name": "TypeScript Debug (ts-node)",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/linked_list/reverse/typescript/reverse_k_group_with_constraints.ts",
            "cwd": "${workspaceFolder}/linked_list/reverse/typescript",
            "runtimeArgs": [
                "-r",
                "ts-node/register"
            ],
            "args": [],
            "console": "integratedTerminal",
            "internalConsoleOptions": "neverOpen",
            "env": {
                "TS_NODE_PROJECT": "${workspaceFolder}/linked_list/reverse/typescript/tsconfig.json"
            },
            "sourceMaps": true,
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Lua Debug",
            "type": "node",
            "request": "launch",
            "program": "lua",
            "args": [
                "${workspaceFolder}/linked_list/reverse/lua/reverse_k_group_with_constraints.lua"
            ],
            "cwd": "${workspaceFolder}/linked_list/reverse/lua",
            "console": "integratedTerminal"
        }
    ]
}